	},
	"peerDependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"@fluentui/react": "^7.121.4",
		"react-dnd": "^11.1.3"
	},
	"devDependencies": {
		"@fluentui/react": "^7.121.8",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"@types/jest": "^24.9.1",
		"@types/node": "^12.12.47",
		"@types/react": "^16.9.41",
		"@types/react-dom": "^16.9.8",
		"@babel/cli": "^7.0.0",
		"babel-preset-react-app": "^9.1.2",
		"react": "^16.13.1",
		"react-dnd": "^11.1.3",
		"react-dom": "^16.13.1",
		"react-scripts": "3.4.1",
		"typescript": "^3.9.6"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "npm run build:lib && npm run build:type",
		"build:app": "react-scripts build",
		"build:lib": "rm -rf dist && NODE_ENV=production babel src/lib --extensions .ts,.tsx,.d.ts --out-dir dist",
		"build:type": "tsc --project tsconfig.types.json",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"publish": "npm run build && npm publish --access public"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"react-dnd-html5-backend": "^11.1.3"
	}
}
